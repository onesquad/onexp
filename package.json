{
  "name": "onexp",
  "version": "0.1.3",
  "description": "The original OneSquad experience",
  "repository": "onesquad/onexp",
  "license": "MIT",
  "author": "onesquad",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "dev": "concurrently \"yarn start\" \"cd example && yarn start\"",
    "prepare": "yarn run build",
    "start": "chokidar \"src/**/*.js\" \"src/**/*.jsx\" \"src/**/*.scss\" --initial -c \"yarn build\"",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,es6,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@blueprintjs/core": "^3.20.0",
    "ramda": "^0.26.1",
    "react-router-dom": "^5.1.2",
    "react-text-transition": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.7.0",
    "@svgr/rollup": "^4.3.3",
    "babel-eslint": "^10.0.3",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-scripts": "^3.2.0",
    "rollup": "^0.68.2",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^3.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.4.1",
    "react-dom": "^15.0.0 || ^16.4.1"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
